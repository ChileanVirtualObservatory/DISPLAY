for line in lines.index:
    plt.axvline(x=line, ymin=0, ymax= 1, color='g')
for column in dictionary_recal.columns:
    plt.plot(Detector.get_freq_index_from_params(), dictionary_recal[column], color='r')
plt.plot(Detector.get_freq_index_from_params(), Detector.detected_gauss, color='b')
# plt.plot(Detector.get_freq_index_from_params(), total, color='k')
plt.show()
